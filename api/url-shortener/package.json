{
	"name": "directus-extension-url-shortener",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^9.16.1"
	},
	"scripts": {
		"build": "directus-extension build",
		"watch": "directus-extension build --watch",
		"dev": "directus-extension build -w --no-minify"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.16.1"
	},
	"dependencies": {
		"tmp-promise": "^3.0.3"
	}
}
